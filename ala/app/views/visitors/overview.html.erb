<table class="table table-striped table-condensed">
  <tr>
    <td><b>subject</b></td>
    <td colspan="4"><b>actions</b></td>
  </tr>
  <% Subject.all.each do |subject| %>
      <tr>
        <td><%= subject.subject %></td>
        <td><%= link_to 'do test', new_practice_path( subject: subject.id ) %></td>
        <td><%= link_to 'do exam', new_exam_path( subject: subject.id ) %></td>
      </tr>
  <% end %>
</table>